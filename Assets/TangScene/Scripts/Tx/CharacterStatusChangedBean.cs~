/**
 * Character status switch bean
 * Date: 2013/11/4
 * Author: zzc
 */

namespace TangScene
{
  public class CharacterStatusSwitchBean
  {
    public long actorId;
    public CharacterStatus before;
    public CharacterStatus after;

    public CharacterStatusSwitchBean(long actorId, CharacterStatus before, CharacterStatus after)
    {
      this.actorId = actorId;
      this.before = before;
      this.after = after;
    }
  }
}